#include <bits/stdc++.h>
 
using namespace std;
 
int main() {
    int cas;
    cin >> cas;
    while (cas--){
    int a,b,c;
    cin >> a >> b >> c;
    if (a == b+c) cout << "YES" << '\n';
    else if (b == a+c) cout << "YES" << '\n';
    else if (c == b+a) cout << "YES" << '\n';
    else cout << "NO" << '\n';    
    }
    return 0;
}